package org.example;

public class Fibonacci {

    public static void main(String[] args){

        int a=0;
        int b=1;
        int term = 9;
        int c;

        for(int i=0;i<=term;i++){
            System.out.println(a);
            c=a+b;
            a=b;
            b=c;

        }
    }

}
