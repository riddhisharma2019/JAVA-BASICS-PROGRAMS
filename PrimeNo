package org.example.javaPrograms;

public class PrimeNo {

    public static void main(String[] args){

        int count=0;
        int num = 3;

        for(int i=1;i<=num; i++){

            if(num%i==0){
                count ++;
            }
        }

        if(count==2){
            System.out.println("it is a prime number...");
        }
        else{
            System.out.println("it is a  not prime numbe...");
        }
    }


}
