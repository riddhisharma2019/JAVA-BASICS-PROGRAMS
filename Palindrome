package org.example.javaPrograms;

public class Palindrome {
    public static void main(String[] args){
        int num = 178;
        int original=num;
        int rev=0;

        while(num>0){
            int digit = num%10;
            rev = rev* 10 + digit;

            num=num/10;
        }

        if(original==rev){
            System.out.println("it is pallindrome");
        }
        else{
            System.out.println("it is not  pallindrome");
        }


    }

}
